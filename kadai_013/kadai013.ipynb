        "#関数\n",
        "def zeikomi(price, tax):\n",
        "\n",
        "#与えられた引数priceとtaxを乗じ、変数totalに代入\n",
        "  total = price +  (price * tax / 100)\n",
        "  return total\n",
        "\n",
        "#関数の呼び出しと結果の出力\n",
        "result = zeikomi(110,10)\n",
        "print(result)"
