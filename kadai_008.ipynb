        "import random\n",
        "\n",
        "var = random.randint(1,999)\n",
        "\n",
        "# 3と5の両方の倍数の場合はFizzBuzz\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "    print(\"FizzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "    print(\"Buzz\")\n",
        "else :\n",
        "    print(var)\n",
        ""
